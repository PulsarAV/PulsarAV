<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="syscoon_financeinterface_search_view" model="ir.ui.view">
        <field name="name">syscoon.financeinterface.searc.view</field>
        <field name="model">syscoon.financeinterface</field>
        <field name="inherit_id" ref="syscoon_financeinterface.syscoon_financeinterface_search_view"/>
        <field name="arch" type="xml">
            <xpath expr="//search" position="inside">
                <filter name="datev_ascii_accounts" domain="[('mode', '=', 'datev_ascii')]" string="DATEV ASCII Accounts"/>
            </xpath>
        </field>
    </record>
</odoo>
