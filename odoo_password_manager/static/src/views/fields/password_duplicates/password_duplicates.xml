<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="odoo_password_manager.PasswordDuplicates" owl="1">
        <div t-if="this.props.value" class="alert alert-danger w-100 password-duplicates-container">
            <i class="fa fa-warning"> </i> This password has
            <span class="text-primary password-duplicates-observe mr4" t-on-click.prevent="() => this._onShowDuplicates()">potential duplicates:</span>
            <span t-esc="this.props.value"/>.
        </div>
    </t>

</templates>
